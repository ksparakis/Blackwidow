{
    "manifest_version": 2,

    "name": "BlackWidow"
    , "description": "This extension scans page for bad Crypto and vulnerable code, to warn developers."
    , "version": "1.0"
    , "background": {
        "scripts": ["resources/jquery-3.2.1.min.js", "popup.js"]
    }
    , "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"]
            , "js": ["resources/jquery-3.2.1.min.js", "background.js"]
                }],

    "browser_action": {
        "default_icon": "imgs/insect.png"
        , "default_popup": "popup.html"
    }



    
    , "permissions": [
    "activeTab"










        
        , "https://ajax.googleapis.com/"
  ]
}